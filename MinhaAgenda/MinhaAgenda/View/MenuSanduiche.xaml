             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MinhaAgenda.View.MenuSanduiche">

    <MasterDetailPage.Master>
        <ContentPage Title="Menu">

            
            <StackLayout BackgroundColor="#A12E57">
                <Button  BackgroundColor="#A12E57" Text="Dashboard"         TextColor="#FFFF"   Clicked="Button_Clicked" />
                <Button  BackgroundColor="#A12E57" Text="Novo Agendamento"  TextColor="#FFFF"   Clicked="Button_Clicked_agendamento"/>
                <Button  BackgroundColor="#A12E57" Text="Cliente"           TextColor="#FFFF"   Clicked="Button_Clicked_2"/>
                <Button  BackgroundColor="#A12E57" Text="Produto/Serviço"   TextColor="#FFFF"   Clicked="Button_Clicked_3"/>
                <Button  BackgroundColor="#A12E57" Text="Financeiro"        TextColor="#FFFF"   Clicked="Button_Clicked_4"/>
            </StackLayout>
        </ContentPage>
    </MasterDetailPage.Master>

    <MasterDetailPage.Detail>
        <NavigationPage>
            
        </NavigationPage>
    </MasterDetailPage.Detail>  
    
    
</MasterDetailPage>